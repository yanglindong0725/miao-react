{
  "name": "miao-react-monorepo",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.0.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "dev": "pnpm --filter lab-quick-start dev",
    "dev:all": "pnpm --parallel --filter \"./apps/**\" dev",
    "build": "pnpm build:packages && pnpm build:apps",
    "build:packages": "pnpm --filter \"./packages/utils\" build && pnpm --filter \"./packages/types\" build && pnpm --filter \"./packages/hooks\" build && pnpm --filter \"./packages/ui\" build",
    "build:apps": "pnpm --filter \"./apps/**\" build",
    "lint": "pnpm --recursive lint",
    "type-check": "pnpm --recursive type-check",
    "clean": "pnpm --recursive exec rm -rf dist node_modules",
    "clean:dist": "pnpm --recursive exec rm -rf dist"
  },
  "devDependencies": {
    "@types/node": "^24.6.0",
    "typescript": "~5.9.3"
  }
}
